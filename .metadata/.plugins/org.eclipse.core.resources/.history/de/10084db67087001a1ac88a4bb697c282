package p1;

public class app {

	public static void main(String[] args) {
	//Primitive data types - Create variables
		int aValue = 42;
		//32 Bit value.Larger range of values.	
		short aShort = 93;
		//16 bit value. Smaller range of values.	
		Long aLong = 8304;
		//64 bit value Very large range of numbers.
		double aDouble = 3.4553;
		//64 bit value. Used to define numbers with decimals.
		float aFloat = 5.243F;
		//32 bit value. Used to define numbers with decimals.Less decimal places, less precise than a double.
		char aChar = 'b';
		//
		boolean aBoolean = true;
		//
		byte aByte = 127;
		//
				
	//Print variables
		System.out.println(aValue);
		//Prints aValue to the console
		
		System.out.println(aShort);
		//Prints aShort to the console
		
		System.out.println(aLong);
		//Prints aLong to the console
		
		System.out.println(aDouble);
		//prints aDouble to the console
		
		System.out.println(aFloat);
		//prints aFloat to the console
		
		System.out.println(aChar);
		//Prints aChar to the console
		
		System.out.println(aBoolean);
		//prints aBoolean to the console
		
		System.out.println(aByte);
		//prints aByte to the console
		
	}
}
